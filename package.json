{
  "name": "ballbox",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean-gradle": "cd android && ./gradlew clean",
    "clean-gradle-cache": "cd android && ./gradlew --stop && ./gradlew cleanBuildCache",
    "clean": "react-native clean-project-auto",
    "pods": "cd ios && pod install",
    "xcode": "open ios/BallBox.xcworkspace",
    "husky-prepare": "husky install && chmod ug+x .husky/*",
    "husky-setup":"npx husky add .husky/pre-commit 'yarn lint-staged'"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint"
    ]
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.1",
    "@react-native-async-storage/async-storage": "^1.15.17",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@ui-kitten/components": "5.1.1",
    "@ui-kitten/date-fns": "^5.1.1",
    "@ui-kitten/eva-icons": "5.1.1",
    "date-fns": "^2.28.0",
    "immer": "^9.0.12",
    "libphonenumber-js": "^1.9.46",
    "react": "^17.0.2",
    "react-native": "^0.67.1",
    "react-native-confirmation-code-field": "^7.3.0",
    "react-native-error-boundary": "^1.1.12",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-svg": "^12.1.1",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@ui-kitten/metro-config": "^5.1.1",
    "babel-jest": "^27.4.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-clean-project": "^4.0.0",
    "react-native-flipper": "^0.128.4",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "^17.0.2",
    "rn-async-storage-flipper": "^0.0.10",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
